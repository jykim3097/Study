<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student3" class="com.javalec6_1.ex1.Student">
		<constructor-arg value="홍길자" />
		<constructor-arg value="27" />
		<constructor-arg>
			<list>
				<value>야구 보기</value>
				<value>스카이 캐슬</value>
			</list>
		</constructor-arg>
		
		<property name="height">
			<value>182</value>
		</property>
		
		<property name="weight" value="85" />
	</bean>
	
	<bean id="family" class="com.javalec6_1.ex1.Family" c:papaName="정아빠" c:mamaName="정엄마" p:sisterName="정누나">
		<property name="brotherName" value="정형" />
	</bean>

</beans>
